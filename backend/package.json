{
	"name": "@routinetactics/backend",
	"version": "1.0.0",
	"main": "dist/index.js",
	"license": "MIT",
	"dependencies": {
		"@babel/preset-env": "^7.1.6",
		"@prisma/client": "^5.1.1",
		"chai": "^4.3.7",
		"express": "^4.18.2",
		"firebase-admin": "^11.10.1",
		"mocha": "^10.2.0"
	},
	"devDependencies": {
		"@types/chai": "^4.3.5",
		"@types/chai-http": "^4.2.0",
		"@types/express": "^4.17.17",
		"@types/mocha": "^10.0.1",
		"@types/node": "^20.4.2",
		"chai-http": "^4.4.0",
		"prisma": "^5.1.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6"
	},
	"scripts": {
		"build": "tsc",
		"start": "node dist/src/index",
		"test": "mocha --require ts-node/register -r dotenv/config --no-cache",
		"test:watch": "mocha --watch --watch-files",
		"dev": "npx tsc --watch & node --watch dist/src/index.js",
		"startDb": "docker-compose up -d",
		"migrateDb": "npx prisma migrate dev --name init",
		"seedDb": "npx prisma db seed",
		"stopDb": "docker-compose down"
	},
	"prisma": {
		"seed": "ts-node prisma/seed.ts"
	}
}
